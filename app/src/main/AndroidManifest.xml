<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.cmviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.test.cmfragment.CMViewPageActivity" />
        <activity android:name="com.test.cmletterview.CMLetterSiderViewActivity" />
        <activity android:name="com.test.cmtablayoutview.CMTabLayoutViewActivity" />
        <activity android:name="com.test.cmtouch.CMTouchViewActivity"></activity>
    </application>

</manifest>